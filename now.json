{
    "name": "insomnia",
    "alias": ["insomnia.kickass.website"],
    "version": 2,
    "builds": [
        {
            "src": "build/**/*",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/build/$1"
        }
    ]
}
